package week3

object rationals {
  val x = new Rational(1,2)                       //> x  : week3.Rational = week3.Rational@44eefb4
  x.numer                                         //> res0: Int = 1
  x.denom                                         //> res1: Int = 2
  
}

class Rational(x: Int, y: Int) {
	def numer = x
	def denom = y
	
	def add(x: Rational, y: Rational) {
		( x.numer + y.numer) / x.denom + y.denom) /  
	}
}